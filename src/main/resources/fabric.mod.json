{
  "schemaVersion": 1,
  "id": "zetamod",
  "version": "${version}",

  "name": "ZetaMod",
  "description": "A mod that does many things!",
  "authors": [
    "TwelvianZeta",
    "Draydenspace_FS (Technical Blocks)"
  ],
  "contact": {
    "homepage": "https://github.com/MicherGit/zetamod",
    "sources": "https://github.com/MicherGit/zetamod"
  },

  "license": "CC0-1.0",
  "icon": "assets/zetamod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "zeta.zetamod.mod.ZetaMod",
      "zeta.zetamod.mod.features.items.CrashItemInitializer",
      "zeta.zetamod.mod.managers.GeneralManager",
      "zeta.zetamod.mod.features.portals.PortalInit",
      "zeta.zetamod.mod.features.keystone.Keystone",
      "zeta.zetamod.mod.features.commands.StartWorld"

    ],
    "client": [
      "zeta.zetamod.mod.client.ZetaModClient"
    ],
    "server": []
  },
  "mixins": [
    "zetamod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.10.0",
    "fabric": "*",
    "minecraft": ">=1.17",
    "customportalapi": ">=0.0.1-beta32-1.17",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener": "zetamod.accesswidener"
