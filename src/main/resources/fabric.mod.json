{
  "schemaVersion": 1,
  "id": "zetamod",
  "version": "${version}",

  "name": "ZetaMod",
  "description": "A mod that does many things!",
  "authors": [
    "ZetaTheEliatrope (obviously)",
    "SuperCoder79 (worldborder code from zetamod.mod.mixins.worldborder)"
  ],
  "contact": {
    "homepage": "https://github.com/MicherGit/Zetamod",
    "sources": "https://github.com/MicherGit/Zetamod"
  },

  "license": "CC0-1.0",
  "icon": "assets/zetamod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "zetamod.mod.ZetaModMain",
      "zetamod.mod.features.items.CrashItemInitializer",
      "zetamod.mod.managers.GeneralManager",
      "me.sargunvohra.mcmods.dataloader.DataLoader"
    ],
    "client": [
      "zetamod.mod.client.ZetaModClient"
    ],
    "cotton-client-commands": [
      "zetamod.mod.features.commands.FarLandsCommand"
    ]
  },
  "mixins": [
    "zetamod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.10.0",
    "fabric": "*",
    "minecraft": "*"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener": "zetamod.accesswidener"
}
