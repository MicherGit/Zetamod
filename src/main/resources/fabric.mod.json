{
  "schemaVersion": 1,
  "id": "zetamod",
  "version": "${version}",

  "name": "ZetaMod",
  "description": "A mod that does many things!",
  "authors": [],
  "contact": {
    "homepage": "https://github.com/MicherGit/zetamod",
    "sources": "https://github.com/MicherGit/zetamod"
  },

  "license": "CC0-1.0",
  "icon": "assets/zetamod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "zeta.zetamod.mod.ZetaModMain",
      "zeta.zetamod.mod.features.items.CrashItemInitializer",
      "zeta.zetamod.mod.managers.GeneralManager",
      "net.kyrptonaught.customportalapi.CustomPortalsMod",
      "zeta.zetamod.mod.features.portals.LushNetherPortal"
    ],
    "client": [
      "zeta.zetamod.mod.client.ZetaModClient",
      "net.kyrptonaught.customportalapi.client.CustomPortalsModClient"
    ],
    "cotton-client-commands": [
      "zeta.zetamod.mod.features.commands.FarLandsCommand"
    ],
    "server": [
      "net.kyrptonaught.customportalapi.networking.NetworkManager"
    ]
  },
  "mixins": [
    "zetamod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.10.0",
    "fabric": "*",
    "minecraft": ">=1.17-alpha.21.16.a"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener": "zetamod.accesswidener"
}
